{
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {
        "document": true,
        "navigator": true,
        "window": true,
        "node": true
    },
    "extends": "react-app",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],

    "rules": {
        "comma-dangle": ["error", "never"],
        "indent": ["error", 4]
    }
}